{
  "name": "react-doctor",
  "version": "0.0.3",
  "bin": {
    "react-doctor": "./dist/cli.cjs"
  },
  "files": [
    "dist"
  ],
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/cli.d.ts",
      "import": "./dist/cli.js",
      "require": "./dist/cli.cjs"
    }
  },
  "scripts": {
    "dev": "tsup --watch",
    "build": "rm -rf dist && NODE_ENV=production tsup",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "commander": "^14.0.3",
    "oxlint": "^1.47.0",
    "picocolors": "^1.1.1"
  },
  "devDependencies": {
    "tsup": "^8.5.1"
  }
}
